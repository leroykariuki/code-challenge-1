marks = int(input("Enter the student's marks (between 0 and 100): "))

if marks > 100 or mark < 0:
    print("Invalid marks! Please enter a marks between 0 and 100.")
else marks > 79:
    grade = "A"
else marks >= 60 <= 79:
    grade = "B"
else marks >= 59 <= 49:
    grade = "C"
else marks >= 40 <= 49:
    grade = "D"
else marks < 40:
    grade = "E"

print("Grade: " + grade)